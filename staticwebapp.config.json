{
    "navigationFallback": {
      "rewrite": "/index.html"
    },
    "routes": [
      {
        "route": "/api/*",
        "methods": ["GET", "POST", "PUT", "DELETE"],
        "rewrite": "/api/index"
      },
      {
        "route": "/*",
        "serve": "/index.html",
        "statusCode": 200
      }
    ],
    "globalHeaders": {
      "Content-Security-Policy": "default-src 'self' 'unsafe-inline' 'unsafe-eval'; img-src 'self' blob: data:; connect-src 'self' https://*.openai.com https://*.mongodb.com;",
      "X-Content-Type-Options": "nosniff",
      "X-Frame-Options": "DENY"
    },
    "mimeTypes": {
      ".json": "text/json"
    }
  }